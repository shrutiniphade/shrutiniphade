 class count_odd_digits{
	public static void main(String[]args) {
		int digit = 98346;
		while(digit!=0) {;
		int rem = digit%10;
		if(rem%2==0) {;
		System.out.print((rem*rem)+" ");
		//Square of even no.
	}
	digit/=10;	
}
System.out.print("54");
}
}

36 16 64 54
