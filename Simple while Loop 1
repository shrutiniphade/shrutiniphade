3]
class countDigit{
	public static void main(String[]args) {
		int digit = 93229572;
		int count = 0;
		while(digit!=0) {;
		     count++;
		     digit=digit/10;
	}
	System.out.print("Count of digit:"+ count);
   }
}

Count of digit:8
