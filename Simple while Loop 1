6]
class sum_of_ Even_mul_of_odd{
	public static void main(String[]args) {
		//int num=6;
		int num=7;
		if(num%2==0) {
			while(num>=1) {
				System.out.print(num+"");
				num--;
				
			}
		}
		else {
			while(num>=1) {
				System.out.print(num+" ");
				num=num-2;
			}
		}
		
	}
}

7 5 3 1 
