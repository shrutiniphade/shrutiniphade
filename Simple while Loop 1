8]
 class reverse No{
	public static void main(String[]args) {
		int No=93229572;
		int rev=0;
		int rem=0;
		int temp=No;
		while(temp !=0) {
			rem= temp%10;
			rev=rev*10+ rem;
			temp=temp/10;
			
		}
		System.out.println(No +"After Reverse: "+ rev);
		
	}
	
	}
O/P=93229572After Reverse: 27592239
-----------------------------------------------------------------------
9]
 class palindrome_no{
	public static void main(String[]args) {
		int num=222;
		int rev =0;
		int temp = num;
		while(temp!=0) {
			rev= rev*10 +(temp%10);
			temp/=10;
			
		}
		if(num==rev) {
			System.out.println(num +"is a palindrome number");
		}else {
			System.out.println(num+"is not palindrom number");
			
		}
		}
	}
O/P=222is a pailndrome number
