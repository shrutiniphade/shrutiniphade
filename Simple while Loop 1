7]
 class primeNo{
	public static void main(String[]arg) {
		//int num=6;
		int num=7;
		if(num%2==0) {
			while(num>=1) {
				System.out.print(num+" ");
				num --;
				
			}
		}
		else {
			while(num>=1){
				System.out.print(num+" ");
				num=num-2;
				
			}
		}
		System.out.println();
	}
}
O/P=7 5 3 1 
