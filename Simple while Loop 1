 class Square_of_even_No{
	public static void main(String[]args) {
		int day =7;
		if(day==0) {
			System.out.println("day assingnment is over");
		}else{
			while(day>=1) {
				System.out.println(day+"day remaning");
				day--;
			}
			if(day==0) {
				System.out.println(day+"days reamaning is over");
				
			}
		}
	}
}
	
7day remaning
6day remaning
5day remaning
4day remaning
3day remaning
2day remaning
1day remaning
0days reamaning is over
